/*
 * Blinker
 * This program will blink at a rate of 10Hz. 
 */

void setup() { //Setup pin 2 as the output.
   pinMode(2, OUTPUT);
}

void loop() {
   digitalWrite(2, HIGH); // LED is on (voltage high)
   delay(100); // wait for 100ms 
   digitalWrite(2, LOW); // LED is off (voltage low)
   delay(100); // wait for 100ms
}
